{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "html": {
      "dependsOn": ["build"],
      "outputs": ["examples/**"]
    },
    "html:tw4": {
      "dependsOn": [],
      "outputs": []
    },
    "tw4:html": {
      "dependsOn": [],
      "outputs": []
    }
  }
}

{
  "name": "@semantic/create-html",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": {
    "name": "buildy-ui",
    "url": "https://github.com/buildy-ui/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildy-ui/semantic-starter.git"
  },
  "scripts": {
    "tw4:html": "bun x @tailwindcss/cli -i ./assets/input-tw4.css -o ./html/styles.css --minify",
    "tw3:html": "bun x -p tailwindcss@3 tailwindcss -i ./assets/input-tw3.css -o ./html/styles.legacy.css --minify -c ./config/tailwind.config.js",
    "html": "bun run tw4:html && bun run tw3:html"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/node": "^22.15.30",
    "glob": "^11.0.3",
    "sharp": "^0.33.0",
    "tailwindcss": "^4.1.9",
    "tailwindcss3": "npm:tailwindcss@3.4.14"
  }
}

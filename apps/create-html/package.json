  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildy-ui/semantic-starter.git"
  },
  "scripts": {
    "tw4:html": "bun x @tailwindcss/cli -i ./assets/input-tw4.css -o ./html/styles.css --minify",
    "analyze:dom": "bun run --bun scripts/analyze-dom.ts",
    "rewrite:semantic": "bun run --bun scripts/rewrite-semantic.ts",
    "compile:route": "bun run --bun scripts/compile-route-css.ts",
    "html:tw4": "bun run tw4:html",
    "html:analyze": "bun run analyze:dom",
    "html:rewrite": "bun run rewrite:semantic",
    "html:compile": "bun run compile:route"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "glob": "^11.0.3",
    "parse5": "^7.2.0",
    "pretty": "^2.0.0",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3",
    "sharp": "^0.33.0",
    "tailwindcss": "^4.1.9"
  },
  "dependencies": {
    "@types/bun": "^1.2.23",
    "@ui8kit/core": "^0.1.8"
  }

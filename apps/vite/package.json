{
  "name": "@ui8kit/vite-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tw4:html": "bun x @tailwindcss/cli -i ./html/input-v4.css -o ./html/styles.css --minify",

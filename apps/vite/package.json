  },
  "dependencies": {
    "@ui8kit/core": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.9",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.9",
    "tailwindcss3": "npm:tailwindcss@3.4.14",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
